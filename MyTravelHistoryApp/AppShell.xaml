<?xml version="1.0" encoding="utf-8" ?>
<Shell
    x:Class="MyTravelHistoryApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:MyTravelHistoryApp.Views"
    Title="My Travel history">

    <ShellContent
        Title="Map"
        ContentTemplate="{DataTemplate views:MapView}"
        Route="MapView" />

    <ShellContent
        Title="History mapview"
        ContentTemplate="{DataTemplate views:HistoryView}"
        Route="HistoryView" />

    <ShellContent
        Title="Dev view"
        ContentTemplate="{DataTemplate views:DevView}"
        Route="DevView" />

</Shell>